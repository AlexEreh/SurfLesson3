<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="tab_text">
        <item name="android:textColor">@color/tab_unselected</item>
        <item name="android:textSize">18sp</item>
        <item name="android:fontFamily">@font/google_sans_bold</item>
    </style>
    <style name="tab_selected_text">
        <item name="android:textColor">@color/tab_selected</item>
        <item name="android:textSize">18sp</item>
        <item name="android:fontFamily">@font/google_sans_bold</item>
    </style>
    <style name="card_launch_text">
        <item name="android:textColor">#FF003D</item>
        <item name="android:textSize">10sp</item>
        <item name="android:fontFamily">@font/google_sans_bold</item>
    </style>
    <style name="card_title_text">
        <item name="android:textColor">@color/black</item>
        <item name="android:textSize">18sp</item>
        <item name="android:fontFamily">@font/google_sans_bold</item>
    </style>
    <style name="card_active_text">
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">8sp</item>
        <item name="lineHeight">11sp</item>
        <item name="android:fontFamily">@font/google_sans_bold</item>
    </style>
    <style name="upcoming_default_text">
        <item name="android:textColor">@color/black</item>
        <item name="android:textSize">16sp</item>
        <item name="android:fontFamily">@font/google_sans_medium</item>
    </style>
    <style name="card_subtitle_text">
        <item name="android:textColor">#8F000000</item>
        <item name="android:textSize">12sp</item>
        <item name="android:fontFamily">@font/google_sans_bold</item>
    </style>
</resources>